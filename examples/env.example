# =============================================================================
# QA MCP Server - Environment Variables
# =============================================================================
# Copy this file to .env in the root directory and fill in your values

# -----------------------------------------------------------------------------
# JIRA Configuration
# -----------------------------------------------------------------------------
JIRA_URL=https://your-company.atlassian.net
JIRA_PERSONAL_TOKEN=your_jira_api_token

# -----------------------------------------------------------------------------
# GitHub Configuration
# -----------------------------------------------------------------------------
GITHUB_HOST=https://api.github.com
GITHUB_TOKEN=ghp_your_github_token
GITHUB_ORG=your-org
GITHUB_REPOS=repo1,repo2,repo3

# -----------------------------------------------------------------------------
# Jenkins Configuration
# -----------------------------------------------------------------------------
JENKINS_URL=https://jenkins.your-company.com
JENKINS_USER=your_username
JENKINS_TOKEN=your_jenkins_api_token

# -----------------------------------------------------------------------------
# AWS Configuration
# -----------------------------------------------------------------------------
AWS_REGION=us-east-1

# -----------------------------------------------------------------------------
# Webex Configuration (Optional)
# -----------------------------------------------------------------------------
WEBEX_TOKEN=your_webex_token

# -----------------------------------------------------------------------------
# AI Client Configuration (Optional)
# -----------------------------------------------------------------------------
AI_SECRET_ID=your_aws_secret_id
AI_AUTH_URL=https://your-auth-provider.com/oauth2/token
AI_AZURE_ENDPOINT=https://your-openai.azure.com
AI_MODEL=gpt-4
